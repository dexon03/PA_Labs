@model NodeValue

<form method="post" asp-action="Edit" class="form">
    <div class="form-group">
        <label>Id </label>
        <input asp-for="NodeValueId" class="form-control" readonly/>
    </div>
    <div class="form-group">
        <label>Value </label>
        <input asp-for="Value" class="form-control" />
    </div>
    <button type="submit">Submit</button>
</form>
<a asp-action="Index" class="btn-primary fs-2" >Back to main page</a>